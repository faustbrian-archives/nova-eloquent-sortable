{
    "name": "kodekeep/nova-eloquent-sortable",
    "description": "A Laravel Nova field for spatie/eloquent-sortable.",
    "keywords": [
        "kodekeep",
        "nova-eloquent-sortable"
    ],
    "homepage": "https://github.com/kodekeep/nova-eloquent-sortable",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "spatie/eloquent-sortable": "^3.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\NovaEloquentSortable\\FieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
